<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <appSettings>
        <add key="api:BaseUrl" value="https://YOUR-API-URL-HERE.com/api" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
        <add key="signalr:ServerUrl" value="https://YOUR-WEB-URL-HERE.com/signalr" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
        <add key="sitemode" value="prod" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    </appSettings>
    <system.web>
        <compilation xdt:Transform="RemoveAttributes(debug)" />
    </system.web>
</configuration>
